## basic predicates
is_gripper
is_object
is_place
is_pose
is_box
is_sphere
is_capsule

## predicates to help formulating rules
on
busy
pushing

## initial state (generated by the code from config file)
START_STATE {}

#####################################################################

DecisionRule gripper_push {
  Obj, Floor, Gripper, 
  { (is_gripper Gripper) (is_object Obj) (on Floor Obj) (is_box Floor) (busy Gripper)! }
  { (on Floor Obj)!  #deletes
    (pushing Gripper Obj Floor) (busy Gripper) #adds
    }
}

#####################################################################

DecisionRule end_push {
  Pusher, Obj, Floor, To,
  { (pushing Pusher Obj Floor) (is_place To) }
  { (pushing Pusher Obj Floor)! (busy Pusher)!  #deletes
    (on To Obj) # adds
    }
}
